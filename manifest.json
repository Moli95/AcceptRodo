{
  "name": "accept-rodo",
  "author": "Tomasz Åšwirski",
  "manifest_version": 2,
  "version": "1.0",
  "description": "Simple Chrome extension which accept RODO form on most popular websities",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "permissions": [
    "declarativeContent",
    "storage",
    "activeTab",
    "tabs",
    "background",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {                   
    "16": "images/favicon-16x16.png",
    "48": "images/favicon-32x32.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/favicon-16x16.png",
      "48": "images/favicon-32x32.png"
    },
    "default_popup": "popup.html"
  }
}
